<!DOCTYPE html>
<html>
<head>

  <%= render "layouts/head" %>

</head>
	
<body>

    <%=  render "shared/fb" %>


     <%= render 'layouts/header'  %>


    <div class="padded_container">



		<section class="round">
			<% flash.each do |key,value|%>
			 	<div class="flash <%= key %>">
				    <%= value %>
				</div>
			<%end%>

        </section>

		<%= yield %>
    </div>

		<%= render 'layouts/footer'  %>

		<%= debug(params) if Rails.env.development? %>

</body>
</html>