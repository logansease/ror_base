<h1>All users</h1>
      
<%= will_paginate%> <!-- note you can tell it the object to paginate as an argument if not by convention -->

<ul class="users"> 

	
	 <%= render @users %>   
	
	<!--  the above line works in place of this:
		<% @users.each do |user| %>  
	        <%=render 'user', :user => user %>
	      <%end%> 
		since rails will enumerate the array and render each with the class name -->
	
</ul> 

<%= will_paginate%>